[*.{js,jsx,ts,tsx,vue}]
indent_style = space
indent_size = 2
trim_trailing_whitespace = true
insert_final_newline = true
ij_any_spaces_within_braces = true
ij_any_spaces_within_parentheses = false
ij_any_spaces_within_brackets = true
ij_any_spaces_within_array_initializer_braces = true
ij_javascript_spaces_within_brackets = true
ij_javascript_spaces_within_object_literal_braces = true